{
    "name": "chaplean/unit-bundle",
    "description": "Contains utilities for functional and unit tests",
    "keywords": ["symfony2"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://chaplean.com/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "~2.8",
        "symfony/monolog-bundle": "~2.4",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "phpunit/phpunit": "^4.8",
        "liip/functional-test-bundle": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "mockery/mockery": "^0.9.4",
        "jms/di-extra-bundle": "^1.6.0",
        "sensio/framework-extra-bundle": "^3.0",
        "jms/security-extra-bundle": "1.*",
        "nelmio/alice": "^2.1"
    },
    "require-dev": {
        "chaplean/coding-standard": "1.1.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "chaplean/mailer-bundle": "^1.0",
        "friendsofsymfony/rest-bundle": "~1.5",
        "jms/serializer-bundle": "^1.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Chaplean\\Bundle\\UnitBundle": "",
            "Foo\\Bundle\\FooBundle": ""
        }
    },
    "target-dir" : "Chaplean/Bundle/UnitBundle"
}
