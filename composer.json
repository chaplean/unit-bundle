{
    "name": "chaplean/unit-bundle",
    "description": "Contains utilities for functional and unit tests",
    "keywords": ["symfony2"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://chaplean.com/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "phpunit/phpunit-selenium": "1.4.*",
        "liip/functional-test-bundle": "~1.0",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "behat/mink": "~1.6",
        "behat/mink-extension": "~2.0",
        "behat/mink-selenium2-driver": "~1.2"
    },
    "require-dev": {
        "liip/functional-test-bundle": "~1.0",
        "doctrine/doctrine-fixtures-bundle": "~2.2"
    },
    "autoload": {
        "psr-0": {
            "Chaplean\\Bundle\\UnitBundle": ""
        }
    },
    "target-dir" : "Chaplean/Bundle/UnitBundle",
    "bin": ["bin/selenium-server-standalone-2.45.0.jar", "bin/chromedriver"]
}
