{
    "name": "chaplean/unit-bundle",
    "description": "Contains utilities for functional and unit tests",
    "keywords": ["symfony2"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://chaplean.com/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.chaplean.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.7.*",
        "symfony/monolog-bundle": "~2.4",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "phpunit/phpunit-selenium": "1.4.*",
        "liip/functional-test-bundle": "~1.0",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "behat/mink": "~1.6",
        "behat/mink-extension": "~2.0",
        "behat/mink-selenium2-driver": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "mockery/mockery": "^0.9.4",
        "jms/di-extra-bundle": "^1.6.0",
        "sensio/framework-extra-bundle": "^3.0",
        "jms/security-extra-bundle": "1.*",
        "nelmio/alice": "^2.1"
    },
    "require-dev": {
        "chaplean/coding-standard": "1.1.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "chaplean/mailer-bundle": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Chaplean\\Bundle\\UnitBundle": ""
        }
    },
    "target-dir" : "Chaplean/Bundle/UnitBundle",
    "bin": ["bin/selenium-server-standalone-2.45.0.jar", "bin/chromedriver"]
}
